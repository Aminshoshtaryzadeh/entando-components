<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">


    <bean id="dashboardDashboardBarChartDAO"
          class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardbarchart.DashboardBarChartDAO">
        <property name="dataSource" ref="servDataSource"/>
    </bean>

    <bean id="dashboardDashboardBarChartManager"
          class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardbarchart.DashboardBarChartManager"
          parent="abstractService">
        <property name="keyGeneratorManager" ref="KeyGeneratorManager"/>
        <property name="dashboardBarChartDAO" ref="dashboardDashboardBarChartDAO"/>
    </bean>

    <bean id="dashboardDashboardBarChartService"
          class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardbarchart.DashboardBarChartService">
        <property name="dashboardBarChartManager" ref="dashboardDashboardBarChartManager"/>
    </bean>

</beans>
