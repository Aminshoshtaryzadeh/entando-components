<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">


	<bean id="dashboardDashboardMapDAO" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardmap.DashboardMapDAO">
		<property name="dataSource" ref="servDataSource" />
	</bean>

	<bean id="dashboardDashboardMapManager" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardmap.DashboardMapManager"
		parent="abstractService">
		<property name="keyGeneratorManager" ref="KeyGeneratorManager" />
		<property name="dashboardMapDAO" ref="dashboardDashboardMapDAO" />
	</bean>
	
    <bean id="dashboardDashboardMapService" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardmap.DashboardMapService" >
        <property name="dashboardMapManager" ref="dashboardDashboardMapManager" />
    </bean>
    
</beans>
