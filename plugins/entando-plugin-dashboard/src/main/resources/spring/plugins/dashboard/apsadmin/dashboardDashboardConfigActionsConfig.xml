<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:context="http://www.springframework.org/schema/context"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
				http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd" >


	<bean id="dashboardDashboardConfigPluginSubMenu" class="com.agiletec.apsadmin.system.plugin.PluginSubMenuContainer" >
		<property name="subMenuFilePath"><value>/WEB-INF/plugins/dashboard/apsadmin/jsp/common/layouts/subMenu.jsp</value></property>
	</bean>

	<bean id="dashboardDashboardConfigFinderAction" scope="prototype" 
			class="org.entando.entando.plugins.dashboard.apsadmin.dashboardconfig.DashboardConfigFinderAction" 
			parent="abstractBaseAction" >
		<property name="dashboardConfigManager" ref="dashboardDashboardConfigManager" />
	</bean>

	<bean id="dashboardDashboardConfigAction" scope="prototype" 
			class="org.entando.entando.plugins.dashboard.apsadmin.dashboardconfig.DashboardConfigAction" 
			parent="abstractBaseAction" >
		<property name="dashboardConfigManager" ref="dashboardDashboardConfigManager" />
	</bean>




 	
</beans>