<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">


	<bean id="dashboardDashboardPieChartDAO" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardpiechart.DashboardPieChartDAO">
		<property name="dataSource" ref="servDataSource" />
	</bean>

	<bean id="dashboardDashboardPieChartManager" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardpiechart.DashboardPieChartManager"
		parent="abstractService">
		<property name="keyGeneratorManager" ref="KeyGeneratorManager" />
		<property name="dashboardPieChartDAO" ref="dashboardDashboardPieChartDAO" />
	</bean>
	
    <bean id="dashboardDashboardPieChartService" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardpiechart.DashboardPieChartService" >
        <property name="dashboardPieChartManager" ref="dashboardDashboardPieChartManager" />
    </bean>
    
</beans>
