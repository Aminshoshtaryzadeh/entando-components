<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">


	<bean id="dashboardDashboardGaugeChartDAO" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardgaugechart.DashboardGaugeChartDAO">
		<property name="dataSource" ref="servDataSource" />
	</bean>

	<bean id="dashboardDashboardGaugeChartManager" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardgaugechart.DashboardGaugeChartManager"
		parent="abstractService">
		<property name="keyGeneratorManager" ref="KeyGeneratorManager" />
		<property name="dashboardGaugeChartDAO" ref="dashboardDashboardGaugeChartDAO" />
	</bean>
	
    <bean id="dashboardDashboardGaugeChartService" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboardgaugechart.DashboardGaugeChartService" >
        <property name="dashboardGaugeChartManager" ref="dashboardDashboardGaugeChartManager" />
    </bean>
    
</beans>
