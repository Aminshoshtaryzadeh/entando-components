<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">


	<bean id="dashboardDashboardDonutChartDAO" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboarddonutchart.DashboardDonutChartDAO">
		<property name="dataSource" ref="servDataSource" />
	</bean>

	<bean id="dashboardDashboardDonutChartManager" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboarddonutchart.DashboardDonutChartManager"
		parent="abstractService">
		<property name="keyGeneratorManager" ref="KeyGeneratorManager" />
		<property name="dashboardDonutChartDAO" ref="dashboardDashboardDonutChartDAO" />
	</bean>
	
    <bean id="dashboardDashboardDonutChartService" class="org.entando.entando.plugins.dashboard.aps.system.services.dashboarddonutchart.DashboardDonutChartService" >
        <property name="dashboardDonutChartManager" ref="dashboardDashboardDonutChartManager" />
    </bean>
    
</beans>
