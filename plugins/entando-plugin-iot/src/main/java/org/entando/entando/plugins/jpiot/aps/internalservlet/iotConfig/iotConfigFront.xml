<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="jpiot_do/FrontEnd/jpiot/IotConfig" namespace="/do/FrontEnd/jpiot/IotConfig" extends="entando-front-default">
		
		<global-results>
			<result name="failure" type="guiFragment">
				<param name="code">jpiot_is_front_IotConfig_error</param>
				<param name="jspLocation">/WEB-INF/plugins/jpiot/aps/jsp/internalservlet/iotconfig/frontend-iotConfig-error.jsp</param>
			</result>
    	</global-results>
		
    	<action name="list" class="jpiotIotConfigFinderFrontEndAction">
    		<result type="guiFragment">
				<param name="code">jpiot_is_front_IotConfig_list</param>
				<param name="jspLocation">/WEB-INF/plugins/jpiot/aps/jsp/internalservlet/iotconfig/frontend-iotConfig-list.jsp</param>
			</result>
			<interceptor-ref name="entandoFrontEndFreeStack"></interceptor-ref>
        </action>
        
    	<action name="search" class="jpiotIotConfigFinderFrontEndAction">
            <result type="guiFragment">
				<param name="code">jpiot_is_front_IotConfig_list</param>
				<param name="jspLocation">/WEB-INF/plugins/jpiot/aps/jsp/internalservlet/iotconfig/frontend-iotConfig-list.jsp</param>
			</result>
			<interceptor-ref name="entandoFrontEndFreeStack"></interceptor-ref>
        </action>
    	
        <action name="new" class="jpiotIotConfigFrontEndAction" method="newIotConfig">
            <result type="guiFragment">
				<param name="code">jpiot_is_front_IotConfig_entry</param>
				<param name="jspLocation">/WEB-INF/plugins/jpiot/aps/jsp/internalservlet/iotconfig/frontend-iotConfig-entry.jsp</param>
			</result>
			<interceptor-ref name="entandoFrontEndFreeStack"></interceptor-ref>
        </action>
    	
        <action name="edit" class="jpiotIotConfigFrontEndAction" method="edit">
            <result type="guiFragment">
				<param name="code">jpiot_is_front_IotConfig_entry</param>
				<param name="jspLocation">/WEB-INF/plugins/jpiot/aps/jsp/internalservlet/iotconfig/frontend-iotConfig-entry.jsp</param>
			</result>
			<result name="input" type="frontRedirectAction">
				<param name="actionName">list</param>
			</result>
        	<interceptor-ref name="entandoFrontEndFreeStack"></interceptor-ref>
        </action>
        
        <action name="save" class="jpiotIotConfigFrontEndAction" method="save">
        	<result name="input" type="guiFragment">
				<param name="code">jpiot_is_front_IotConfig_entry</param>
				<param name="jspLocation">/WEB-INF/plugins/jpiot/aps/jsp/internalservlet/iotconfig/frontend-iotConfig-entry.jsp</param>
			</result>
			<result type="frontRedirectAction">
				<param name="actionName">list</param>
			</result>
        	<interceptor-ref name="entandoFrontEndFreeValidationStack"></interceptor-ref>
        </action>
		
    	<action name="trash" class="jpiotIotConfigFrontEndAction" method="trash">
            <result type="guiFragment">
				<param name="code">jpiot_is_front_IotConfig_trash</param>
				<param name="jspLocation">/WEB-INF/plugins/jpiot/aps/jsp/internalservlet/iotconfig/frontend-iotConfig-trash.jsp</param>
			</result>
			<result name="input" type="frontRedirectAction">
				<param name="actionName">list</param>
			</result>
        	<interceptor-ref name="entandoFrontEndFreeStack"></interceptor-ref>
        </action>
        
    	<action name="delete" class="jpiotIotConfigFrontEndAction" method="delete">
        	<result type="frontRedirectAction">
				<param name="actionName">list</param>
			</result>
        	<interceptor-ref name="entandoFrontEndFreeStack"></interceptor-ref>
        </action>
    	
    </package>
   
</struts>