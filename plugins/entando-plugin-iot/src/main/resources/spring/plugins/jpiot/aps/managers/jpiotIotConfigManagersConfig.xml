<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">


	<bean id="jpiotIotConfigDAO" class="org.entando.entando.plugins.jpiot.aps.system.services.iotconfig.IotConfigDAO">
		<property name="dataSource" ref="servDataSource" />
	</bean>

	<bean id="jpiotIotConfigManager" class="org.entando.entando.plugins.jpiot.aps.system.services.iotconfig.IotConfigManager"
		parent="abstractService">
		<property name="keyGeneratorManager" ref="KeyGeneratorManager" />
		<property name="iotConfigDAO" ref="jpiotIotConfigDAO" />
	</bean>
	
    <bean id="jpiotIotConfigService" class="org.entando.entando.plugins.jpiot.aps.system.services.iotconfig.IotConfigService" >
        <property name="iotConfigManager" ref="jpiotIotConfigManager" />
    </bean>
    
</beans>
